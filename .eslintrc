{
  "parser": "babel-eslint",
  "rules": {
    "no-unused-expressions": [0],
    "no-underscore-dangle": [0],
    "quote-props": [2],
    "no-multi-spaces": [0],
    "no-unused-vars": [0],
    "no-loop-func": [0],
    "new-cap": [0],
    "key-spacing": [0],
    "max-len": [2],
    "strict": [0],
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "curly": [0],
    "jsx-quotes": 1
  },
  "settings": {
    "ecmascript": 6
  },
  "env": {
    "browser": true,
    "node": true
  }
}
